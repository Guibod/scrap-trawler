{
  "name": "eventlink-scrapper",
  "displayName": "Eventlink scrapper",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Guillaume Boddaert",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "format": "biome format --write",
    "lint": "biome check",
    "prepare": "husky"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.7.1",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "plasmo": "0.90.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "biome": "^0.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "manifest": {
    "permissions": [
      "storage",
      "cookies",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://eventlink.wizards.com/*"
    ]
  }
}
