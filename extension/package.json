{
  "name": "extension",
  "manifest_version": 3,
  "displayName": "Scrap Trawler",
  "version": "0.17.0",
  "description": "A browser extension for extracting event data from EventLink tournaments.",
  "author": "Guillaume Boddaert",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Guibod/scrap-trawler.git"
  },
  "bugs": {
    "url": "https://github.com/Guibod/scrap-trawler/issues"
  },
  "homepage": "https://github.com/Guibod/scrap-trawler#readme",
  "keywords": [
    "plasmo",
    "browser-extension",
    "event-scraper",
    "graphql",
    "magic-the-gathering",
    "eventlink",
    "mtg",
    "tournament"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "format": "biome format --write",
    "lint": "biome check",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "prepare": "husky",
    "sync-version": "tsx src/resources/sync-version.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@faker-js/faker": "^9.6.0",
    "@heroicons/react": "^2.2.0",
    "@heroui/button": "^2.2.10",
    "@heroui/card": "^2.2.10",
    "@heroui/react": "^2.7.2",
    "@heroui/system": "^2.4.7",
    "@heroui/table": "^2.2.9",
    "@heroui/theme": "^2.4.6",
    "@heroui/tooltip": "^2.2.11",
    "@plasmohq/messaging": "^0.7.1",
    "@react-stately/data": "^3.12.3",
    "@visx/axis": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/text": "^3.12.0",
    "bowser": "^2.11.0",
    "clsx": "^2.1.1",
    "consola": "^3.4.0",
    "dexie": "^4.0.11",
    "fastest-levenshtein": "^1.0.16",
    "fflate": "^0.8.2",
    "framer-motion": "^12.6.2",
    "fuse.js": "^7.1.0",
    "graphql-request": "^6.1.0",
    "json-stream-es": "^1.2.1",
    "lucide-react": "^0.483.0",
    "motion": "^12.6.3",
    "papaparse": "^5.5.2",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-aria": "^3.37.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^7.4.0",
    "tailwind-merge": "^3.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@golevelup/ts-vitest": "^0.5.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "0.0.313",
    "@types/gapi": "^0.0.47",
    "@types/gapi.auth2": "^0.0.61",
    "@types/gapi.client": "^1.0.8",
    "@types/geojson": "^7946.0.16",
    "@types/node": "22.14.0",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitest/coverage-v8": "^3.0.8",
    "autoprefixer": "^10.4.20",
    "biome": "^0.3.3",
    "blob-polyfill": "^9.0.20240710",
    "eslint": "^9.23.0",
    "eslint-config-biome": "^1.9.4",
    "eslint-plugin-biome": "^0.9.19",
    "fake-indexeddb": "^6.0.0",
    "fetch-blob": "^4.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "svgo": "^3.0.2",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.19.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "vitest-mock-extended": "^3.1.0"
  },
  "manifest": {
    "manifest_version": 3,
    "key": "$CRX_PUBLIC_KEY",
    "name": "Scrap Trawler BETA",
    "description": "Extract event data from EventLink tournaments. THIS EXTENSION IS FOR BETA TESTING",
    "permissions": [
      "storage",
      "cookies",
      "activeTab",
      "identity"
    ],
    "host_permissions": [
      "https://eventlink.wizards.com/*",
      "https://www.googleapis.com/*",
      "https://docs.google.com/*",
      "https://apis.google.com/*"
    ],
    "oauth2": {
      "client_id": "933041103309-312keqir0ejl8d74oqbd5r6jgtccnql5.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/drive.file"
      ]
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }
}
